{
  "name": "inhabit-react",
  "version": "0.0.0-development",
  "description": "",
  "keywords": [],
  "source": "src/inhabit-react.tsx",
  "main": "dist/inhabit-react.js",
  "module": "dist/inhabit-react.mjs",
  "unpkg": "dist/inhabit-react.umd.js",
  "typings": "dist/inhabit-react.d.ts",
  "files": [
    "dist"
  ],
  "author": "rand0me <not.randome@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/rand0me/inhabit-react"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "microbundle && typedoc --out docs --target es6 --theme minimal --mode file src",
    "start": "microbundle watch",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "precommit": "lint-staged",
    "travis-deploy-once": "travis-deploy-once",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "{src,test}/**/*.{ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "functions": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts,tsx}"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@storybook/addon-actions": "^5.2.3",
    "@storybook/addon-links": "^4.1.9",
    "@storybook/addon-storyshots": "^4.1.9",
    "@storybook/addons": "^4.1.9",
    "@storybook/react": "^4.1.9",
    "@types/jest": "^24.0.6",
    "@types/node": "^12.0.0",
    "@types/react": "^16.8.4",
    "@types/react-dom": "^16.8.2",
    "@types/react-test-renderer": "^16.8.1",
    "@types/storybook__react": "^4.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "colors": "^1.3.2",
    "commitizen": "^3.0.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^3.0.0",
    "jest": "^23.6.0",
    "jest-config": "^23.6.0",
    "lint-staged": "^9.0.0",
    "lodash.camelcase": "^4.3.0",
    "microbundle": "^0.11.0",
    "prettier": "^1.14.3",
    "prompt": "^1.0.0",
    "react-dom": "^16.8.1",
    "react-test-renderer": "^16.8.1",
    "replace-in-file": "^4.0.0",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.13.3",
    "travis-deploy-once": "^5.0.9",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.1",
    "typedoc": "^0.14.2",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "react": "^16.8.2"
  }
}
